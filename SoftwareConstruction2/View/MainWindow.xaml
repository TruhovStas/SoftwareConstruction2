        <Button Content="Запись в текстовый файл" HorizontalAlignment="Left" Margin="10,131,0,0" VerticalAlignment="Top" Click="Button_Click_3" Width="159"/>
        <Button Content="Создать случайный объект" HorizontalAlignment="Left" Margin="10,82,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.938,-10.659" Click="Button_Click" Width="159"/>
        <Button Content="Очистить" HorizontalAlignment="Left" Margin="569,82,0,0" VerticalAlignment="Top" Click="Button_Click_5"/>
        <Button Content="Добавить объект" HorizontalAlignment="Left" Margin="10,181,0,0" VerticalAlignment="Top" Width="159" Click="Button_Click_6"/>

    </Grid>
</Window>
